<% provide(:title, "Edit Member") %> 
<h1>Update member information</h1>

<div class="row">
  <div class="span6 offset3">
	<%= form_for(@member) do |f| %>
  	<%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>
    <%= f.submit "Save changes", class: "btn btn-primary" %>
<% end %>
	</div>
</div>
